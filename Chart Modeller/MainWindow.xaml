<Window x:Class="Chart_Modeller.MainWindow"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chart_Modeller"
        mc:Ignorable="d"
        Title="MainWindow" Height="1024 " Width="1440"  WindowStartupLocation="CenterScreen">
    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
        <Grid VerticalAlignment="Top" Background="White" Height="105"></Grid>
        <TextBlock x:Name="pageName" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,30,0,0" FontSize="40" Foreground="#131C26"/>
        <Button x:Name="backButton" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,0,0" Click="backButton_Click" Foreground="#131C26">Назад</Button>
        <Grid VerticalAlignment="Bottom" Background="#131C26" Height="70" Panel.ZIndex="-1"></Grid>
    </Grid>
</Window>
